{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["packages/*"],
    "name": "@molitio/web-engine",
    "entryPointStrategy": "packages",
    "includeVersion": true,
    "logLevel": "Verbose",
    "excludeNotDocumented": true,
    "exclude": [
        ".yarn/**/*",
        "packages/data-core/src/**/*",
        "packages/system-schema/src/**/*",
        "packages/ui-core-test/src/**/*",
        "packages/ui-core__deprecated/src/**/*",
        "packages/ui-storybook__deprecated/src/**/*",
        "packages/system-schema/src/**/*",
        "packages/john-glenn/src/**/*"
    ],
    "out": "docs",
    "compilerOptions": {
        "jsx": "react-jsx"
    }
}
