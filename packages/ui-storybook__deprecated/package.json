        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^5.2.2",
        "watch": "^1.0.2",
        "webpack": "^5.88.2"
    },
    "stableVersion": "1.0.2"
