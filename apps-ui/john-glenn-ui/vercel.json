{
    "framework": null,
    "ignoreCommand": "git diff HEAD^ HEAD --quiet -- ./apps-ui/john-glenn-ui",
    "buildCommand": "yarn build-john-glenn-ui",
    "outputDirectory": "apps-ui/john-glenn-ui/.next",
    "installCommand": "yarn install --immutable --immutable-cache --check-cache",
    "devCommand": "yarn dev",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "query",
                    "key": "authorized"
                }
            ],
            "headers": [
                {
                    "key": "x-authorized",
                    "value": "true"
                }
            ]
        }
    ]
}
